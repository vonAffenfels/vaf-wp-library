{
  "name": "vonaffenfels/vaf-wp-library",
  "type": "project",
  "description": "Library to simplify plugin development",
  "homepage": "https://github.com/vonAffenfels/vaf-wp-library",
  "authors": [
    {
      "name": "Christoph Friedrich",
      "email": "christoph.friedrich@vonaffenfels.de",
      "homepage": "https://www.vonaffenfels.de"
    }
  ],
  "support": {
    "issues": "https://github.com/vonAffenfels/vaf-wp-library/issues"
  },
  "keywords": [
    "composer",
    "wordpress",
    "wp",
    "vonaffenfels",
    "vaf",
    "library"
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "VAF\\WP\\Library\\": "src/"
    }
  },
  "scripts": {
    "test": [
      "phpcs"
    ]
  },
  "require": {
    "php": ">=8.1",
    "symfony/config": "^6.2",
    "symfony/dependency-injection": "^6.2",
    "symfony/yaml": "^6.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6"
  }
}
